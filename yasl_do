#!yasl

{
	print
	"\n"
	print
} print_nl

{
	print_nl
	0 exit
} print_exit

{
	print_nl
	1 exit
} print_error

!!	// deletes size and first argument

# 3 != ?("Wrong number of arguments !" print_error)

// evaluate numerical arguments as numbers and then sets operator to top
eval
# ^
eval
# ^

"+" %== ?(!! + print_exit) : !
"-" %== ?(!! - print_exit) : !
"*" %== ?(!! * print_exit) : !
"/" %== ?(!! / print_exit) : !
"%" %== ?(!! % print_exit) : !
"<" %== ?(!! < print_exit) : !
">" %== ?(!! > print_exit) : !
"<=" %== ?(!! <= print_exit) : !
">=" %== ?(!! >= print_exit) : !
"==" %== ?(!! == print_exit) : !
"!=" %== ?(!! != print_exit) : !

"Wrong operator !" print_error
