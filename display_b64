	4:&:4
	3:=:4
	4:+:4
	4:+:4
	2:!
	1:=:5
	255:5
	5:&:5
	4:=:5
	5:+:5
	5:+:5
	5:print
} print_rgb

{
	0:1
	1:8
	8:=:8
	8:@
	(
		9:=:9
		1:=:9
		9:]#[:7
		7:=:7
		0:=:7
		7:==:8 ? (0:8) : (1:8 1:1 1:+:1)
		8:=:8
	)
} decode

